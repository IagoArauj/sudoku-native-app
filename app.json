{
  "expo": {
    "name": "Sudoku",
    "slug": "sudoku-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-icon.png",
    "scheme": "sudokuapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.sudoku-app",
      "icon": {
        "dark": "./assets/icons/ios-icon.png",
        "light": "./assets/icons/ios-icon.png",
        "tinted": "./assets/icons/ios-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/icons/adaptative-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.sudokuapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
